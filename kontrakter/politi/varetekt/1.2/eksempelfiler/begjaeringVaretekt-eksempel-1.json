  "vedlegg": {
    "siktelse": {
      "punkt": [
        {
          "nummer": "I",
          "prinsipalPunkt": {
            "lovbud": [
              {
                "lovbudStreng": "Straffeloven § 232 annet ledd første straffalternativ, jf. § 231 første ledd, jf. straffeloven § 27",
                "gjengivelse": "for ulovlig å ha tilvirket, innført, utført, ervervet, oppbevart, sendt eller overdratt narkotika. \r\n\r\nOvertredelsen omfatter en meget betydelig mengde.\r\n\r\nNår et straffebud er overtrådt av noen som har handlet på vegne av et foretak, kan foretaket straffes."
              }
            ],
            "grunnlag": [
              {
                "grunnlagstekst": "Campingbil med kokeutstyr og Breaking Bad DVD'er",
